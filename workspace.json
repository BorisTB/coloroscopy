          }
        }
      }
    },
    "packages-validation": {
      "root": "libs/packages/validation",
      "sourceRoot": "libs/packages/validation/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
